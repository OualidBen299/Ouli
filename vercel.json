{
	"version": 2,
	"builds": [
		{
			"src": "api/app.py",
			"use": "@vercel/python",
			"config": {
				"includeFiles": [
					"api/**/*"
				],
				"excludeFiles": [
					"**/*.txt",
					"**/*.log",
					"**/*.md",
					"**/__pycache__/*",
					"**/*.pyc",
					"**/*.pyo",
					"**/*.jpg",
					"**/*.jpeg",
					"**/*.png",
					"**/*.gif",
					"**/*.bmp",
					"**/*.tiff",
					"**/*.svg",
					"**/*.wav",
					"**/*.mp3",
					"**/*.mp4",
					"**/*.avi",
					"**/*.mov",
					"**/*.flv",
					"**/*.wmv",
					"**/*.webm",
					"**/*.zip",
					"**/*.gz",
					"**/*.tar",
					"**/*.rar",
					"**/*.7z",
					"**/*.exe",
					"**/*.dll",
					"**/*.so",
					"**/*.dylib",
					"**/*.pdf"
				]
			}
		}
	],
	"routes": [
		{"src": "/api/CrossCaptcha", "dest": "api/app.py"}
	]
}
