{
	"version": 2,
	"builds": [
		{
			"src": "api/app.py",
			"use": "@vercel/python",
			"config": {
				"includeFiles": [
					"api/**/*"
				],
				"excludeFiles": [
					"**/*.txt",
					"**/*.log",
					"**/*.md",
					"**/__pycache__/*",
					"**/*.pyc",
					"**/*.pyo"
				]
			}
		}
	],
	"routes": [
		{"src": "/api/CrossCaptcha", "dest": "api/app.py"}
	]
}
